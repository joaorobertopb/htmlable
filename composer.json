{
    "name": "joaorobertopb/htmlable",
    "type": "library",
    "description": "Simple PHP library for abstraction and rendering of html elements.",
    "keywords": [
        "joaorobertopb",
        "htmlable",
        "html"
    ],
    "homepage": "https://github.com/joaorobertopb/htmlable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jo√£o Roberto",
            "email": "joaorobertopb@gmail.com",
            "homepage": "https://github.com/joaorobertopb",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7 || ^6.0 || ^7.0 || ^8.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "JoaoRobertoPB\\Htmlable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoaoRobertoPB\\Htmlable\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "php-cs-fixer fix --using-cache=no --diff --dry-run .",
        "fix-style": "php-cs-fixer fix --using-cache=no ."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
